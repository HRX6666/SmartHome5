<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--扩散圆颜色-->
    <attr name="circle_out_color" format="color"/>
    <!--中心圆颜色-->
    <attr name="circle_in_Color" format="color"/>
    <!--中心圆图片-->
    <attr name="circle_in_image" format="reference"/>
    <!--中心圆半径-->
    <attr name="circle_radius" format="float"/>
    <!--扩散圆宽度,值越小越宽-->
    <attr name="circle_width" format="integer"/>
    <!--最大扩散宽度-->
    <attr name="max_width" format="integer"/>
    <!--扩散速度，值越大越快-->
    <attr name="speed" format="integer"/>
    <attr name="radius" format="dimension"></attr>
    <attr name="backgroundColor" format="color"></attr>
    <attr name="primaryVolumeColor" format="color"></attr>
    <attr name="volumeColor" format="color"></attr>
    <attr name="borderWidth" format="dimension"></attr>
    <attr name="maxVolume" format="integer"></attr>

    <declare-styleable name="CircleView">
        <attr name="circle_out_color"/>
        <attr name="circle_in_Color"/>
        <attr name="circle_in_image"/>
        <attr name="circle_radius"/>
        <attr name="circle_width"/>
        <attr name="max_width"/>
        <attr name="speed"/>
    </declare-styleable>
    <declare-styleable name="StepSeekbar">
        <attr name="seekbarHeight" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="spaceBetween" format="dimension" />
        <attr name="cursorBackground" format="reference" />
        <attr name="markTextArray" format="reference" />
        <attr name="textColorNormal" format="color" />
        <attr name="textColorSelected" format="color" />
        <attr name="seekbarColorNormal" format="color" />
        <attr name="seekbarColorSelected" format="color" />
        <attr name="autoMoveDuration" format="integer" />
    </declare-styleable>
    <!--空调面板自定义属性-->
    <declare-styleable name="AirBoardView">
        <!--中心圆-->
        <attr name="centerCircleRadius" format="dimension" />
        <attr name="centerCircleBackground" format="color" />

        <!--环形进度-->
        <attr name="progressWidth" format="dimension" />
        <attr name="progressStartColor" format="color" />
        <attr name="progressCenterColor" format="color" />
        <attr name="progressEndColor" format="color" />
        <attr name="progressBackgroundColor" format="color" />

        <!--外部圆环-->
        <attr name="outsideCircleRadius" format="dimension" />
        <attr name="outsideCircleBackgroundColor" format="color" />

        <!--温度显示-->
        <attr name="tempTextColor" format="color" />
        <attr name="tempTextSize" format="dimension" />

        <!--刻度条-->
        <attr name="scaleTextColor" format="float" />
        <attr name="scaleTextSize" format="dimension" />

        <attr name="tempStrArray" format="reference" />
    </declare-styleable>
    <declare-styleable name="ArcSeekBar">
        <attr name="left_padding" format="dimension"/>
        <attr name="right_padding" format="dimension"/>
        <attr name="arc_seek_bar_alpha" format="float"/>
        <attr name="indicator_color" format="color"/>
        <attr name="arc_seek_bar_background" format="color"/>
    </declare-styleable>
    <declare-styleable name="RoundImageView">
        <attr name="radius_v" format="reference|dimension" />
    </declare-styleable>
    <declare-styleable name="SelfTextView">
        <attr name="titlesText" format="string"/>
        <attr name="titlesTextColor" format="color"/>
        <attr name="titlesTextSize" format="dimension"/>
    </declare-styleable>



</resources>